# Production Environment Variables
# Copy these to your Railway/Render environment variables panel

# Database - Railway will auto-provide DATABASE_URL
DATABASE_URL=${DATABASE_URL}

# Security - GENERATE NEW SECRET KEY FOR PRODUCTION!
SECRET_KEY=CHANGE_THIS_TO_RANDOM_SECRET_KEY_IN_PRODUCTION
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Application
APP_NAME=Expense Tracker API
APP_VERSION=1.0.0
DEBUG=False

# CORS - Update with your actual frontend domain
ALLOWED_ORIGINS=https://your-app.vercel.app,https://www.your-app.com

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
